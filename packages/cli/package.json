{
	"name": "@every-plugin/cli",
	"version": "0.1.0",
	"description": "CLI for managing every-plugin development lifecycle",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"type": "module",
	"bin": "./dist/index.mjs",
	"exports": "./dist/index.mjs",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"stub": "unbuild --stub",
		"dev": "tsx ./src/index.ts",
		"start": "node ./dist/index.mjs"
	},
	"publishConfig": {
		"access": "public",
		"executableFiles": [
			"./dist/index.mjs"
		]
	},
	"keywords": [
		"every-plugin",
		"cli",
		"module-federation",
		"plugin"
	],
	"devDependencies": {
		"@types/bun": "latest",
		"@types/degit": "^2.8.6",
		"@types/node": "^24.3.1",
		"tsx": "^4.20.5",
		"unbuild": "^3.6.1"
	},
	"peerDependencies": {
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"@types/fs-extra": "^11.0.4",
		"c12": "^3.2.0",
		"chalk": "^5.6.2",
		"commander": "^14.0.0",
		"degit": "^2.8.4",
		"fs-extra": "^11.3.1",
		"get-tsconfig": "^4.10.1",
		"semver": "^7.7.2",
		"tinyexec": "^1.0.1",
		"yocto-spinner": "^1.0.0",
		"zod": "^4.1.7"
	}
