    "effect-ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elliotBraem/potlock-campaigns-indexer.git"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@effect/language-service": "^0.57.1",
