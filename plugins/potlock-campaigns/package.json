    "@rspack/cli": "^1.5.2",
    "@rspack/core": "^1.5.2",
    "@types/node": "^24.6.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "every-plugin": "workspace:*",
    "msw": "^2.3.0",
    "sirv": "^3.0.2",
