{
  "name": "@curatedotfun/telegram",
  "version": "0.0.1",
  "description": "Telegram source plugin for curate.fun",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "peerDependencies": {
    "every-plugin": "^0.3.0",
    "typescript": "^5.6.0"
  },
  "devDependencies": {
    "@effect/language-service": "^0.41.1",
    "@module-federation/node": "^2.7.17",
    "@rspack/cli": "^1.5.2",
    "@rspack/core": "^1.5.2",
    "@types/node": "^24.6.0",
    "dotenv": "^17.2.2",
    "every-plugin": "workspace:*",
    "msw": "^2.3.0",
    "sirv": "^3.0.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "zephyr-rspack-plugin": "^0.0.59"
  },
  "scripts": {
    "build": "rspack build && tsc -p tsconfig.build.json",
